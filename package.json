{
  "name": "gfc_tickets",
  "version": "1.0.0",
  "private": true,
  "description": "Utility bot for the GFC server",
  "author": "Alex",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "shx rm -rf ./dist/ && tsc --project tsconfig.json",
    "start": "npm run build && cross-env DOTENV_CONFIG_PATH=./.env.production ts-node -r dotenv/config dist/index.js",
    "dev": "npm run build && cross-env DOTENV_CONFIG_PATH=./.env.dev ts-node -r dotenv/config dist/index.js"
  },
  "dependencies": {
    "@sapphire/framework": "^5.5.0",
    "@sapphire/plugin-subcommands": "^7.0.1",
    "@sapphire/utilities": "^3.18.2",
    "axios": "1.12.0",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "mongoose": "^9.1.3"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "shx": "^0.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CommandLineFox/GFC_Tickets.git"
  },
  "bugs": {
    "url": "https://github.com/CommandLineFox/GFC_Tickets/issues"
  },
  "homepage": "https://github.com/CommandLineFox/GFC_Tickets#readme"
}
